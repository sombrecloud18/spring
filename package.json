{
  "name": "spring-project",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/dom": "^10.4.0",
    "classnames": "^2.5.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.0",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules react-scripts test",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules react-scripts test --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "testMatch": [
      "**/src/**/*.test.js",
      "**/__tests__/**/*.test.js"
    ],
    "moduleNameMapper": {
      "^redux-mock-store$": "<rootDir>/node_modules/redux-mock-store"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "3.2.5",
    "redux-mock-store": "^1.5.5"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  }
}
